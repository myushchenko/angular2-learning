{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "rimraf dist && tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "build": "rimraf dist && tsc -p app",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "test": "karma start karma.conf.js"
  },
